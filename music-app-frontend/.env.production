# 前端生产环境配置

# ===== 应用基础配置 =====
VITE_APP_TITLE=音乐应用
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_LANGUAGE=zh-CN

# ===== 环境模式配置 =====
VITE_DEV_MODE=false
VITE_DEBUG=false

# ===== API服务配置 =====
VITE_API_BASE_URL=https://music-app-backend-production.onrender.com/api
VITE_SOCKET_URL=https://music-app-backend-production.onrender.com

# ===== 文件上传配置 =====
VITE_MAX_FILE_SIZE=100MB
VITE_ALLOWED_FILE_TYPES=.mp3,.wav,.flac,.m4a

# ===== 功能特性配置 =====
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
